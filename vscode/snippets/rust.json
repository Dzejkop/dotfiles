{
    "Print type size": {
        "prefix": "type_size",
        "body": [
            "println!(\"std::mem::size_of::<{}>() = {}\", stringify!($1), std::mem::size_of::<$1>());"
        ]
    },
    "Trace debug variable": {
        "prefix": "trace",
        "body": [
            "dbg!(&$1);"
        ]
    },
    "Parse Unwrap": {
        "prefix": "pu",
        "body": [
            "$1.parse::<$2>().unwrap()"
        ]
    },
    "Test mod": {
        "prefix": "test module",
        "body": [
            "#[cfg(test)]",
            "mod tests {",
            "    use super::*;",
            "",
            "    $1",
            "}"
        ]
    },
    "Public Struct": {
        "prefix": "pstruct",
        "body": [
            "pub struct $1 {",
            "    $2",
            "}"
        ]
    },
    "Public Enum": {
        "prefix": "penum",
        "body": [
            "pub enum $1 {",
            "    $2",
            "}"
        ]
    },
    "Newtype": {
        "prefix": "newtype",
        "body": [
            "pub struct $1($2);"
        ]
    }
}